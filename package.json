{
  "name": "syrupy-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "biome check --write ./src",
    "lint:ci": "biome ci ./src",
    "format": "biome format --write ./src",
    "format:check": "biome format ./src",
    "check": "biome check --write ./src && tsc --noEmit",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "generate": "plop",
    "generate:component": "plop component",
    "generate:entity": "plop entity",
    "generate:route": "plop route"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "biome check --write --no-errors-on-unmatched --diagnostic-level=error",
      "vitest related --run --passWithNoTests"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@hookform/resolvers": "^3.1.1",
    "@tanstack/react-query": "^5.22.2",
    "@tanstack/react-router": "^1.133.32",
    "@tanstack/router-devtools": "^1.133.32",
    "@tanstack/router-vite-plugin": "^1.133.32",
    "clsx": "^1.2.1",
    "daisyui": "^5.3.10",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.0",
    "react-toastify": "^10.0.0",
    "tailwind-merge": "^2.2.2",
    "zod": "^3.21.4",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@tailwindcss/forms": "^0.5.3",
    "@tanstack/react-query-devtools": "^5.28.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.11.13",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^4.0.4",
    "autoprefixer": "^10.4.14",
    "happy-dom": "^20.0.8",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.6",
    "plop": "^4.0.4",
    "postcss": "^8.4.21",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.9.3",
    "vite": "^6.1.0",
    "vitest": "^4.0.4"
  }
}
